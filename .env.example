# PostgreSQL configuration
POSTGRES_USER=
POSTGRES_PASSWORD=
DATABASE_URL=
LITELLM_DATABASE_URL=

WEBUI_SECRET_KEY=
OLLAMA_BASE_URL=http://ollama:11434
OLLAMA_BASE=http://ollama:11434
OLLAMA_API_BASE=http://ollama:11434

# LiteLLM configuration
LITELLM_MASTER_KEY=
LITELLM_SALT_KEY=
LITELLM_LOG=INFO
LITELLM_API_KEY=

# Optional: Enable detailed logging
# SET_VERBOSE: "True"

# Add your API keys here as environment variables
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
# COHERE_API_KEY: your_cohere_api_key_here
STORE_MODEL_IN_DB=true
# OpenAI API configuration (pointing to LiteLLM)
OPENAI_API_BASE_URL=http://litellm:4000/v1

# Optional: Disable authentication for development
# WEBUI_AUTH: False

# Optional: Set admin email
# WEBUI_ADMIN_EMAIL=

# Enable vector database features
ENABLE_RAG_HYBRID_SEARCH=true
ENABLE_RAG_WEB_LOADER_SSL_VERIFICATION=false

# Chroma DB settings (if you want to use Chroma instead of PostgreSQL for vectors)
# CHROMA_DB_URL: http://chromadb:8000

# File upload settings
ENABLE_IMAGE_GENERATION=true
ENABLE_COMMUNITY_SHARING=false

# Ollama settings
# Optional: Set Ollama host
# OLLAMA_HOST=ollama
# # Optional: Enable GPU support (uncomment if you have NVIDIA GPU)
# # NVIDIA_VISIBLE_DEVICES: all

ADMINER_DEFAULT_DB=postgres
DMINER_DEFAULT_SERVER=myai_pgdb
